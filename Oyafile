Project: TutFront

Values:
  port: 3000
  api: 'http://localhost:4000'

install: |
  npm install

test: |
  ./node_modules/mocha/bin/mocha

start: |
  api=${Oya[api]}
  port=${Oya[port]}
  API=$api PORT=$port node index.js

onPush: |
  oya run install
  oya run test

onMergeToMaster: |
  oya run install
  # oya run deploy
